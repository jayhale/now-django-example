{
    "version": 2,
    "name": "now-django-example",
    "builds": [{
        "src": "index.py",
        "use": "@ardent-labs/now-python-wsgi",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes" : [{
        "src" : "/(.*)", "dest":"/"
    }],
    "env":{
        "DATABASE_URL":"<INSERT CONNECTION STRING HERE>",
    }
}
